{
  "name": "Hop-hop-hop",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.0"
  },
  "description": "Dames chinoises en ligne",
  "main": "home.js",
  "scripts": {
    "start": "node server.js",
    "test": "ava \"./tests/*.test.js\" --verbose",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "selenium": "java -jar ./helpers/selenium-server-standalone-3.5.3.jar",
    "ui-test": "./node_modules/.bin/wdio ./helpers/wdio.conf.js"
  },
  "ava": {
    "require": [
      "./helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "mysql": "^2.16.0",
    "nyc": "^12.0.2",
    "pug": "^2.0.3",
    "regex": "^0.1.1",
    "rewire": "^4.0.1",
    "selenium-standalone": "^6.15.1",
    "sinon": "^6.1.4",
    "socket.io": "^1.7.4",
    "wdio-mocha-framework": "^0.6.2",
    "webdriverio": "^4.13.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "browser-env": "^3.2.5",

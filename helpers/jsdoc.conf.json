{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": "./",
        "exclude": [".git", "node_modules", ".nyc_output", "coverage", "documentation", "helpers"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./documentation/",
        "recurse": true
    }
}
